# 1.ornek:
liste=[4,45,67,8,4,789,64,36]
while True:
    try:
        number=float(input("listede var mı= "))
        for m in liste:
            if m==number:
                print(True)
            else:
                print(False)
                break 
    except:
        print("listede sadece float deger bulunuyor. lutfen uygun deger gırınız...")
        
        




# 2.ornek:
kullanıcıKayıt={
"kahraman":"kjdhfj23",
"bayram":"eıuryfh987",
"neval":"897nsdfn"    
    }    

def KayıtlıMı():
    kullanıcı=input("kullanıcı adını giriniz= ")
    şifre=input("şifrenizi giriniz= ")
    try:
        if kullanıcıKayıt[kullanıcı]==şifre:
            return "kullanıcı kayıtlı.."
        else:
            return "kullanıcı şifresini yanlış girdiniz.."
    except:
         print("kullanıcı bulunamadı. lutfen var olan bir kullanıcı gırınız..")
         
         
    
print(KayıtlıMı())





